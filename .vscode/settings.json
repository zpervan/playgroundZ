{
    "actionButtons": {
        "defaultColor": "#ff0034", // Can also use string color names.
        "loadNpmCommands": false, // Disables automatic generation of actions for npm commands.
        "reloadButton": "♻️", // Custom reload button text or icon (default ↻). null value enables automatic reload on configuration change
        "commands": [
            {
                "name": "Linear Regression | Build", // Current project Build task
                "color": "yellow",
                "command": "bazel build //Main/LinearRegression:main"
            },
            {
                "name": "Linear Regression | Run",
                "color": "yellow",
                "command": "bazel run //Main/LinearRegression:main",
            },
            {
                "name": "Bazel Clean",
                "color": "yellow",
                "command": "bazel clean --expunge",
            },
            {
                "name": "Bazel Test All",
                "color": "yellow",
                "command": "bazel test --cache_test_results=0 --test_output=errors //...",
            }
        ]
    },
